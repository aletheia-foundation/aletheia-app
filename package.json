{
  "name": "aletheia-app",
  "version": "1.0.0",
  "description": "Decentralised science",
  "main": "main.js",
  "scripts": {
    "startOld": "./node_modules/.bin/truffle migrate && electron .",
    "testOld": "./node_modules/.bin/truffle test && ./node_modules/.bin/truffle migrate && mocha",
    "ethereum-local": "testrpc",
    "ipfs-local": "node ./scripts/ipfs-local.js",
    "ethereum-testnet": "./scripts/start-geth-testnet.sh",
    "ethereum-testnet:attach": "./scripts/attach-geth-testnet.sh",
    "ng": "ng",
    "lint": "ng lint",
    "start": "webpack --watch",
    "start:web": "webpack-dev-server --target web --content-base . --port 4200 --inline",
    "build:electron:main": "tsc main.ts --outDir dist && copyfiles package.json dist && cd dist && npm install --prod && cd ..",
    "build": "webpack --display-error-details && npm run build:electron:main",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "electron:serve": "npm run build && electron ./dist --serve",
    "electron:test": "electron ./dist",
    "electron:dev": "npm run build && electron ./dist",
    "electron:prod": "npm run build:prod && electron ./dist",
    "electron:linux": "npm run build:prod && node package.js --asar --platform=linux --arch=x64",
    "electron:windows": "npm run build:prod && node package.js --asar --platform=win32 --arch=ia32",
    "electron:mac": "npm run build:prod && node package.js --asar --platform=darwin --arch=x64",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet && npm run build",
    "e2e": "protractor ./protractor.conf.js"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Aletheia",
    "Decentralised",
    "Academic",
    "Publishing",
    "Ipfs"
  ],
  "author": "aletheia-foundation",
  "license": "LGPL-3.0",
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/compiler-cli": "5.1.3",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.9",
    "@types/core-js": "0.9.36",
    "@types/jasmine": "2.5.54",
    "@types/node": "7.0.7",
    "autoprefixer": "7.1.4",
    "bs58": "4.0.1",
    "codelyzer": "4.0.2",
    "copyfiles": "1.2.0",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "cssnano": "3.10.0",
    "electron": "1.7.10",
    "electron-packager": "10.1.1",
    "electron-reload": "1.2.2",
    "ethereumjs-testrpc": "3.9.2",
    "expect.js": "0.3.1",
    "exports-loader": "0.6.4",
    "file-loader": "0.11.2",
    "html-loader": "0.5.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "json-loader": "0.5.7",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "^2.0.9",
    "less-loader": "4.0.5",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "mocha": "3.4.2",
    "postcss-loader": "2.0.6",
    "postcss-url": "7.1.2",
    "protractor": "5.1.2",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.6",
    "script-loader": "0.7.0",
    "sinon": "1.17.7",
    "solc": "0.4.11",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.2",
    "stylus-loader": "3.0.1",
    "truffle": "3.3.2",
    "truffle-config": "1.0.0",
    "truffle-contract": "2.0.1",
    "truffle-expect": "0.0.3",
    "ts-node": "^4.1.0",
    "tslint": "5.7.0",
    "typescript": "2.5.2",
    "url-loader": "0.5.9",
    "web3-fake-provider": "0.1.0",
    "webdriver-manager": "12.0.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  },
  "dependencies": {
    "@angular/animations": "^5.1.3",
    "@angular/common": "5.1.3",
    "@angular/compiler": "5.1.3",
    "@angular/core": "5.1.3",
    "@angular/forms": "5.1.3",
    "@angular/http": "5.1.3",
    "@angular/platform-browser": "5.1.3",
    "@angular/platform-browser-dynamic": "5.1.3",
    "@angular/router": "5.1.3",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-beta.8",
    "@types/ipfs": "git+https://github.com/zabirauf/ipfs-types.git",
    "angular2-notifications": "^0.9.7",
    "bootstrap": "4.0.0-beta.3",
    "config": "1.26.1",
    "core-js": "2.4.1",
    "enhanced-resolve": "^4.0.0-beta.4",
    "go-ipfs-dep": "^0.4.13",
    "ipfs-api": "17.2.6",
    "jquery": "3.2.1",
    "popper.js": "1.12.9",
    "q": "1.5.0",
    "request-promise-native": "1.0.4",
    "rxjs": "5.5.0",
    "truffle-contract": "2.0.1",
    "truffle-expect": "0.0.3",
    "web3": "0.18.4",
    "zone.js": "0.8.17"
  }
}
